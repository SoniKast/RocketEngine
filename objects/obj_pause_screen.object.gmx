            <kind>1</kind>
            <string>/// This script is used to pause the game! 

//Check if we press enter:
if(global.do_time == false) exit;

if(keyboard_check_pressed(global.ip_button_s))
{
    if(pause == false &amp;&amp; pause_bg = noone)
    {
        // Pause Audio:
        audio_pause_all();
        
        // Pause Sound Effect:
        bv_audio_play(_general_pause, global.sfx_volume);
        
        // Set Pause to True:
        pause = true;
        
        //Create a Screenshot of the current view:
        pause_bg = background_create_from_surface(application_surface, 0, 0, global.display_width, global.display_height, false, false);
        
        //Deactivate all Instances:
        instance_deactivate_all(1);       
    }
    // Pause
    else 
    {
        if(pause == true)
        {
            // Set pause to false:
            pause = false;
             
            // Activate all Instances:
            instance_activate_all();
             
            // Resume Audio:
            audio_resume_all(); 
            
            // Clear Screen:
            background_delete(pause_bg);
            pause_bg = noone;
        }
    }
}
</string>
          </argument>
