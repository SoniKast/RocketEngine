        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Modes
switch(initialized)
{
    case 0:
    {
        if(keyboard_check_pressed(global.ip_button_s) || joy_pressed(0, global.dip_button_s))
        {
            initialized = 1;
            bv_audio_play(_general_select, global.sfx_volume);
        }
        break;
    }
    case 1:
    {
        if(menu_active == 1)
        {
            if(keyboard_check_pressed(global.ip_down) || (joy_hat_y(0) = 1 &amp;&amp; gmp.dpad_down == 0))
            {
                if(menu_option &lt; 4)
                {
                    menu_option += 1;
                }
                else
                {
                    menu_option = 0;
                }
                bv_audio_play(_general_score_add, global.sfx_volume); 
            }
            if(keyboard_check_pressed(global.ip_up) || (joy_hat_y(0) = -1 &amp;&amp; GMP.dpad_up == 0))
            {
                if(menu_option &gt; 0)
                {
                    menu_option -= 1;
                }
                else
                {
                    menu_option = 4;
                }
                bv_audio_play(_general_score_add, global.sfx_volume); 
            }
            
            if(keyboard_check_pressed(global.ip_button_a) || joy_pressed(0, global.dip_button_a))
            {
                menu_active = 0;
                switch(menu_option)
                {
                    case 0:
                    {
                        if(initialized = 1){
                            with(instance_create(0, 0, obj_fade_to))
                            {
                                fade_to = rm_character_select;
                            }
                            bv_audio_play(_general_select, global.sfx_volume);
                        }
                        break;
                    }
                    case 1:
                    {
                        menu_active = 1;
                        bv_audio_play(_general_error, global.sfx_volume);
                        break;
                    }
                    case 2:
                    {
                        menu_active = 1;
                        bv_audio_play(_general_error, global.sfx_volume);
                        break;
                    }
                    case 3:
                    {
                        with(instance_create(0, 0, obj_fade_to))
                        {
                            fade_to = rm_options_menu;
                        }
                        bv_audio_play(_general_select, global.sfx_volume);
                        break;
                    }
                    case 4:
                    {
                        menu_active = 0;
                        game_end();
                        break;
                    }
                }
            }
        }
        break;
    }
}
</string>
